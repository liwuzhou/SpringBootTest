<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lee.demo.dao.SalaryDao">


    <update id="updateSalaryByUserId" parameterType="java.lang.String">
        update t_salary set salary = #{salary} where user_id = #{userId}
    </update>
</mapper>